{
  "name": "n8n-nodes-random",
  "version": "1.0.0",
  "description": "Random number generator using Random.org for n8n",
  "keywords": [
  "n8n-community-node-package",
  "random",
  "number", 
  "generator",
  "random.org"
  ],
  "license": "MIT",
  "homepage": "https://github.com/JoaquimGCVS/n8n-random-connector",
  "author": {
    "name": "Joaquim Vilela"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/JoaquimGCVS/n8n-random-connector.git"
  },
  "main": "index.js",
  "scripts": {
  "build": "tsc && mkdir dist\\Random\\nodes\\Random 2>nul && copy nodes\\Random\\random.svg dist\\Random\\nodes\\Random\\random.svg",
  "dev": "tsc --watch",
  "test": "jest",
  "test:watch": "jest --watch"
},
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [],
    "nodes": [
     "dist/Random/Random.node.js"
    ]
  },
  "devDependencies": {
    "@types/node": "^22.0.0",
    "n8n-workflow": "^1.85.4",
    "typescript": "^5.9.2",
    "@types/jest": "^29.5.0",
    "jest": "^29.5.0",
    "ts-jest": "^29.1.0"
  },
  "peerDependencies": {
    "n8n-workflow": "^1.85.4"
  }
  
}

